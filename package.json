{
  "name": "mcqmcp-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "MCQMCP - Assessment infrastructure for AI tutoring",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "npm run dev -w packages/server",
    "dev:website": "npm run dev -w packages/website",
    "build:server": "npm run build -w packages/server",
    "build:website": "npm run build -w packages/website",
    "build": "npm run build:server && npm run build:website"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JDerekLomas/MCQMCP.git"
  },
  "author": "Derek Lomas",
  "license": "MIT"
}
